<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
        xmlns:ma="urn:import:gwt.material.design.addins.client"
		xmlns:in="urn:import:gwt.material.design.incubator.client"
	
	>
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialContainer>
		<m:MaterialRow>
			<m:MaterialColumn >
    		<in:toggle.GroupToggleButton ui:field="varcharOps" />
    	 </m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialRow>
			<m:MaterialColumn grid="s12">
            <ma:emptystate.MaterialEmptyState ui:field="progress" iconType="STORAGE" iconColor="PURPLE" />

				<ma:combobox.MaterialComboBox ui:field="valueset" placeholder="Choisir..." visible="false"/>
			</m:MaterialColumn>
		</m:MaterialRow>
	</m:MaterialContainer>
</ui:UiBinder> 