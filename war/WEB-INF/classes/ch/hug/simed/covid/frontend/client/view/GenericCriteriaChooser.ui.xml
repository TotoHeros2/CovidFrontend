<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
		xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client"
	>
	<ui:style>
	.bold {
		font-weight: bold;
	}
	</ui:style>
		<m:MaterialContainer offset="s1" padding="10">
			<m:MaterialLabel ui:field="resourceName" addStyleNames="{style.bold}" text="Not found"/>
		
			<m:MaterialRow>
				<m:MaterialColumn grid="s3" visible="true">
					<m:MaterialLabel  text="Resource intermédiaire 0"/>
				</m:MaterialColumn>
				<m:MaterialColumn grid="s3" visible="false">
					<m:MaterialLabel  text="Intermédiaire 2"/>
				</m:MaterialColumn>
				<m:MaterialColumn grid="s3">
					<m:MaterialLabel  text="Field"/>
				</m:MaterialColumn>				
			</m:MaterialRow>
			
			<m:MaterialRow>
				<m:MaterialColumn grid="s3" visible="true">
				<m:MaterialListValueBox ui:field="listInterm0"></m:MaterialListValueBox>
				</m:MaterialColumn>
				<m:MaterialColumn grid="s3" visible="false">
				<m:MaterialListValueBox ui:field="listInterm1"></m:MaterialListValueBox>
				</m:MaterialColumn>
				<m:MaterialColumn grid="s3">
				<m:MaterialListValueBox ui:field="listFields"></m:MaterialListValueBox>
				</m:MaterialColumn>				
			</m:MaterialRow>
			
						<m:MaterialLabel ui:field="selected"  text="No selection"/>
			
		</m:MaterialContainer>
</ui:UiBinder> 