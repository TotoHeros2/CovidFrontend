<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:p="urn:import:ch.hug.simed.covid.frontend.client.view"
	xmlns:in="urn:import:gwt.material.design.incubator.client"
	
	>
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
	
     <m:MaterialPanel shadow="3" padding="20" margin="30">

          <!-- Navbar component that contains a search icon -->
          <m:MaterialNavBar visible="false" ui:field="navBar" paddingLeft="20" width="100%" textColor="WHITE">
            <m:MaterialNavBrand ui:field="navBrand" text="Recherche de critère"/>
            <m:MaterialLink ui:field="btnSearch" iconType="SEARCH" iconColor="WHITE" float="RIGHT"/>
          </m:MaterialNavBar>
          
            <!-- NavBar Search -->
 		<m:MaterialNavBar ui:field="navBarSearch" width="100%" visible="true">
 			<p:SmartMaterialSearch ui:field="txtSearch" placeholder="Débutez votre recherche" backgroundColor="WHITE" iconColor="BLACK" active="true" shadow="1"/>
		 </m:MaterialNavBar>
  		
  		        
     </m:MaterialPanel>

	</g:HTMLPanel>
</ui:UiBinder> 