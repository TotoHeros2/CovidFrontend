<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:in="urn:import:gwt.material.design.incubator.client"
		xmlns:m="urn:import:gwt.material.design.client.ui"
	
	>
	<ui:style>
	.important {
		font-weight: bold;
	}
	.active {
   	 background: green;
    	color: #fff;
	}
	.group-toggle-button button.active {
   	 	background: #66bb6a;
    	color: #fff;
	}
	</ui:style>
	<g:HTMLPanel>
	<m:MaterialRow>
	<m:MaterialColumn grid="s4" offset="s4">
    <in:toggle.GroupToggleButton ui:field="groupToggle">
     </in:toggle.GroupToggleButton>
     </m:MaterialColumn>
	</m:MaterialRow>
	</g:HTMLPanel>
</ui:UiBinder> 